#. [side]: type = EoMa_Mastermage; id = Mastermage; 
#. [message]: speaker = narrator; 
#: To_Lands_Unknown/scenarios/08_Bridge_of_Light.cfg: 53
#: To_Lands_Unknown/scenarios/18_The_Last_Summoner.cfg: 69
#: To_Lands_Unknown/scenarios/21_Deep_Freeze.cfg: 102
#: To_Lands_Unknown/scenarios/21_Deep_Freeze.cfg: 115
#: To_Lands_Unknown/scenarios/21_Deep_Freeze.cfg: 125
#: To_Lands_Unknown/scenarios/21_Deep_Freeze.cfg: 131
#: To_Lands_Unknown/scenarios/21_Deep_Freeze.cfg: 142
msgid "Mage"
msgstr ""

#. [message]: speaker = Mage; 
#: To_Lands_Unknown/scenarios/08_Bridge_of_Light.cfg: 99
msgid "Surrender! You have no chance to win; you are alone."
msgstr ""

#. [message]: speaker = Rashti; 
#: To_Lands_Unknown/scenarios/08_Bridge_of_Light.cfg: 104
msgid "You do not understand, human..."
msgstr ""

#. [message]: speaker = Rashti; 
#: To_Lands_Unknown/scenarios/08_Bridge_of_Light.cfg: 109
msgid "You cannot..."
msgstr ""

#. [message]: speaker = Rashti; 
#: To_Lands_Unknown/scenarios/08_Bridge_of_Light.cfg: 114
msgid "...DEFEAT ME!!!"
msgstr ""

#. [message]: speaker = Mage; 
#: To_Lands_Unknown/scenarios/08_Bridge_of_Light.cfg: 171
msgid "...Hmmm... Interesting..."
msgstr ""

#. [message]: speaker = Mehir; 
#: To_Lands_Unknown/scenarios/08_Bridge_of_Light.cfg: 176
msgid "Wow!"
msgstr ""

#. [message]: speaker = Rashti; 
#: To_Lands_Unknown/scenarios/08_Bridge_of_Light.cfg: 192
msgid "...Oh! A summoner. You have come to help me, have you not?"
msgstr ""

#. [message]: speaker = Mehir; 
#: To_Lands_Unknown/scenarios/08_Bridge_of_Light.cfg: 196
msgid "I... I... Yes. I am here to find Tashkar, the leader of Tar-Tabar. And you must be Rashti’rhami - goddess of all Rhamis, right? It's an honour to meet you."
msgstr ""

#. [message]: speaker = Rashti; 
#: To_Lands_Unknown/scenarios/08_Bridge_of_Light.cfg: 201
msgid "Yes. And now..."
msgstr ""

#. [message]: speaker = Rashti; 
#: To_Lands_Unknown/scenarios/08_Bridge_of_Light.cfg: 206
msgid "...AT THEM!!!"
msgstr ""

#. [message]: speaker = Mage; 
#: To_Lands_Unknown/scenarios/08_Bridge_of_Light.cfg: 210
msgid "Defend the entrance. They mustn’t get inside!"
msgstr ""

#. [message]: speaker = Mehir; 
#: To_Lands_Unknown/scenarios/08_Bridge_of_Light.cfg: 215
msgid "I’ve seen their kind before. These troopers look like those who attacked al-Kamija... I have a bad feeling about this..."
msgstr ""

#. [objective]: condition = win; 
#: To_Lands_Unknown/scenarios/08_Bridge_of_Light.cfg: 220
#: To_Lands_Unknown/scenarios/15_Far_North.cfg: 562
msgid "Defeat all enemies"
msgstr ""

