msgid "Protect lizards"
msgstr ""

#. [message]: speaker = Rhaxiz; 
#: To_Lands_Unknown/scenarios/04_Southern_Nations.cfg: 726
msgid "Khaal siftu nax. Rav!"
msgstr ""

#. [message]: speaker = Mehir; 
#: To_Lands_Unknown/scenarios/04_Southern_Nations.cfg: 737
msgid "We did it! Now it’s time to defeat all barbarians."
msgstr ""

#. [message]: speaker = Rhaxiz; 
#: To_Lands_Unknown/scenarios/04_Southern_Nations.cfg: 741
msgid "(series of gestures demanding retreat)"
msgstr ""

#. [message]: speaker = Mehir; 
#: To_Lands_Unknown/scenarios/04_Southern_Nations.cfg: 745
msgid "You want to withdraw, but where?"
msgstr ""

#. [message]: speaker = Rhaxiz; 
#: To_Lands_Unknown/scenarios/04_Southern_Nations.cfg: 749
msgid "(points his finger towards a keep inside the jungle)"
msgstr ""

#. [message]: speaker = Mehir; 
#: To_Lands_Unknown/scenarios/04_Southern_Nations.cfg: 754
msgid "So this is your plan! Protect the lizard leader!"
msgstr ""

#. [objective]: condition = win; 
