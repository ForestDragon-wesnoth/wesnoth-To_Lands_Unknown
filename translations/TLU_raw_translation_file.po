msgstr ""

#. [message]: speaker = Rhaxiz; 
#: To_Lands_Unknown/scenarios/04_Southern_Nations.cfg: 1011
msgid "Shatizakaaaaiii!"
msgstr ""

#. [message]: speaker = narrator; 
#: To_Lands_Unknown/scenarios/04_Southern_Nations.cfg: 1020
msgid "Brogh’ur’s dead! Run!!!"
msgstr ""

#. [message]: speaker = Mehir; 
#: To_Lands_Unknown/scenarios/04_Southern_Nations.cfg: 1057
msgid "We did it! They are defeated!"
msgstr ""

#. [message]: speaker = Mehir; 
#: To_Lands_Unknown/scenarios/04_Southern_Nations.cfg: 1075
