#. [side]: type = EoMa_Platinum_Warrior; id = Dedylos; 
#: To_Lands_Unknown/scenarios/15_Far_North.cfg: 71
#: To_Lands_Unknown/scenarios/16_Gates_of_Kharos.cfg: 151
msgid "Dedylos"
msgstr ""

#. [side]: 
#: To_Lands_Unknown/scenarios/15_Far_North.cfg: 95
msgid "team_name^Hydras"
msgstr ""

#. [side]: 
#: To_Lands_Unknown/scenarios/15_Far_North.cfg: 108
